<div class="container">
    <div class="col-lg-12">
        <p id="notice"><%= notice %></p>
        <h1>Images</h1>
        <p> Click on an image to annotate</p>
    </div>
    <div class="row">
        <% @images.each do |image| %>
            <div class="col-lg-3 col-md-4 col-xs-6 thumb">
                <%= link_to  image_tag(image.url, width: "200px", class: "img-responsive" ), image_path(image)%>
            </div>
        <% end %>
    </div>

</div>
<%= link_to 'New Image', new_image_path %>
<br>

